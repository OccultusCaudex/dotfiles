! urxvt

URxvt*geometry:                53x20
urxvt*letterSpace: 0
!URxvt*font: -*-gohufont-*-*-*--10-*-*-*-*-*-*-*
!URxvt*font:  xft:DejaVu Sans Mono:pixelsize=10
!URxvt*font: -*-clean-*-*-*--12-*-*-*-*-*-*-*
!URxvt*font: -*-terminus-*-*-normal-*-12-*-*-*-*-60-*-*
URxvt*font: xft: Misc Tamsyn:pixelsize=11
!URxvt*font: xft:GohuFont:pixelsize=11
!URxvt*font: -*-tewii-*-*-*-*-12-*-*-*-*-*-*-*
URxvt*depth:                32
!URxvt.background:            [85]#0d0d0d
URxvt*background: rgba:0000/0000/0200/c800
URxvt*foreground:            [100]#7b7b7b
URxvt*borderLess:   False
URxvt*scrollBar:    False
URxvt*cursorBlink:   True
!URxvt*transparent:   True
!URxvt*shading: 30
URxvt*tintColor:   #494949
!URxvt*tintColor: #6d9e3f
URxvt*saveLines:  5000
URxvt*termName:    rxvt-unicode
URxvt*fading:   10
URxvt.perl-ext-common: default,matcher
URxvt.urlLauncher: firefox 
URxvt.matcher.button: 2
URxvt.perl-ext-common: default,matcher
URxvt.url-launcher: /usr/bin/xdg-open
URxvt.matcher.button: 1

! Add tabs
URxvt.perl-ext-common: ...,tabbed,...
! Set terminal padding
URxvt.internalBorder: 10

! Enables copy/paste with Ctrl-Shift-C/V
URxvt.iso14755: false
URxvt.iso14755_52: false
URxvt.perl-ext-common: default,clipboard
URxvt.keysym.Shift-Control-C: perl:clipboard:copy
URxvt.keysym.Shift-Control-V: perl:clipboard:paste

